koordinat = [(0, 1), (1, 1), (2, 1), (3, 1), (4, 1), (5, 1), (5, 2), (5, 3), (5, 
4), (5, 5), (6, 5), (6, 6), (6, 7), (6, 8), (6, 9), (7, 9), (8, 6), (8, 7), (8, 8), (8, 9)]
koordx=[i[0] for i in koordinat]
koordy=[j[1] for j in koordinat]
koordxy=koordx+koordy
print(koordx)
print(koordy)
print(koordxy)
